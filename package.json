    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.2",
    "jest": "22.4.3",
    "np": "2.20.1",
    "npm-check": "5.9.0",
    "rimraf": "2.6.3",
    "run-sequence": "2.2.1"
